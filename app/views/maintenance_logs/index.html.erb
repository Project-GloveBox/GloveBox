<h1>Maintenance Log History<h1>

<% @maintenance_logs.each do |maintenance_log| %>
  <% maintenance_log.maintenance_actions.each do |maintenance_action| %>
     <%= maintenance_action.type %>
     <%= maintenance_action.service_date %>
     <%= maintenance_action.mileage %>
     <%= maintenance_action.cost %>
     <%= link_to 'show', maintenance_action %><br>
   <%end%>
<%end%> 